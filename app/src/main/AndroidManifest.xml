<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ganko.movil.ganko1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.login.LoginActivity"
            android:theme="@style/NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.farm.MainActivity" />
        <activity
            android:name="ganko.movil.ganko1.ui.login.password.PasswordActivity"
            android:theme="@style/NoActionBar" />
        <activity android:name=".ui.farm.add.AddFarmActivity" />
        <activity android:name=".ui.menu.MenuActivity" />
        <activity android:name=".ui.inventory.detail.DetailBovineActivity" />
        <activity android:name=".ui.inventory.out.OutBovineActivity" />
        <activity android:name=".ui.inventory.add.AddBovineActivity"></activity>
    </application>

</manifest>